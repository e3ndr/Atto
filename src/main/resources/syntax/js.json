{
    "aliases": [
        "json",
        "js",
        "ts"
    ],
    "override": "GRAY",
    "rules": [
        {
            "match": "(\\/\\/)[\\S].*",
            "color": "GREEN",
            "priority": 1
        },
        {
            "match": "(?=(\\/\\*))[\\s\\S]*(?<=(\\*\\/))",
            "color": "GREEN",
            "priority": 2
        },
        {
            "match": "([\\\"'`])(?:(?=(\\\\?))\\2.)*?\\1",
            "color": "LIGHT_RED",
            "priority": 3
        },
        {
            "match": "(class|function|const|let|var)",
            "color": "CYAN",
            "priority": 4
        },
        {
            "match": "[0-9]+(\\.[0-9]+)?",
            "color": "LIGHT_YELLOW",
            "priority": 5
        },
        {
            "match": "(true|false)",
            "color": "LIGHT_GREEN",
            "priority": 6
        }
    ]
}